{
    "Comment": "DIY StackSets",
    "StartAt": "GetAccounts",
    "States": {
        "GetAccounts": {
            "Type": "Task",
            "Resource": "${GetAccountListArn}",
            "Next": "DeployStacks"
        },
        "DeployStacks": {
            "Type": "Map",
            "InputPath": "$.accounts",
            "ItemsPath": "$.list",
            "MaxConcurrency": 0,
            "Iterator": {
                "StartAt": "DeployStack",
                "States": {
                    "DeployStack": {
                        "Type": "Task",
                        "Resource": "${DeployStackArn}",
                        "End": true
                    }
                }
            },
            "End": true
        }
    }
}